{
  "defaultEnv": "env-default",
  "env-default": {
    "driver": "mysql",
    "host": {
      "ENV": "MYSQL_HOST"
    },
    "user": {
      "ENV": "MYSQL_USER"
    },
    "password": {
      "ENV": "MYSQL_PASS"
    },
    "database": {
      "ENV": "MYSQL_DB"
    },
    "multipleStatements": true,
    "ssl": {
      "ENV": "MYSQL_SSL"
    }
  }
}
